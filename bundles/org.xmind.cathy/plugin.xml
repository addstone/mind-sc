   <extension
         point="org.eclipse.e4.ui.css.core.elementProvider">
      <provider
            class="org.xmind.cathy.internal.css.XSWTElementProvider">
         <widget
               class="org.eclipse.ui.forms.widgets.FormText">
         </widget>
         <widget
               class="org.eclipse.ui.forms.widgets.ScrolledForm">
         </widget>
         <widget
               class="org.xmind.ui.tabfolder.MTabBar">
         </widget>
         <widget
               class="org.xmind.ui.tabfolder.MTabFolder">
         </widget>
      </provider>
