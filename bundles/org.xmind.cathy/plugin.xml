         </command>
      </menuContribution>
      <menuContribution
            allPopups="false"
            locationURI="menu:help?after=group.main">
            <command
                  commandId="org.eclipse.ui.help.helpContents"
                  id="helpContents"
                  label="%menu.help.xmindHelp.label"
