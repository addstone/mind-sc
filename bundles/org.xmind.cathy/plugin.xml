<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="org.xmind.cathy.internal.CathyApplication">
         </run>
      </application>
   </extension>
   <extension
         id="product"
         point="org.eclipse.core.runtime.products">
      <product
            application="org.xmind.cathy.application"
            name="%xmind.product.name">
         <property
               name="windowImages"
               value="icons/xmind.16.png,icons/xmind.32.png,icons/xmind.48.png,icons/xmind.64.png,icons/xmind.128.png,icons/xmind.256.png"/>
         <property
               name="appName"
               value="XMind">
         </property>
         <!--
         <property
               name="preferenceCustomization"
               value="plugin_customization.ini">
         </property>
         -->
          <property
          		name="introTitle"
          		value="Welcome to XMind 2014"/>
          <property
          		name="introBrandingImage"
          		value="product:icons/introimage.png"/>
          <property
          		name="introBrandingImageText"
          		value="XMind 2014"/>
		  <property
				name="introDescription-overview"
				value=""/>
		  <property
				name="introDescription-tutorials"
				value=""/>
		  <property
				name="introDescription-samples"
				value=""/>
          <property
                name="aboutImage"
                value="icons/about.png">
	      </property>
          <property name="aboutText" value="%productBlurb">
          </property>
          <property
                name="applicationXMI"
                value="platform:/plugin/org.xmind.cathy/Application.e4xmi">
          </property>
          <property
                name="applicationCSSResources"
                value="platform:/plugin/org.xmind.cathy/images/">
          </property>
          <property
                name="cssTheme"
                value="org.xmind.cathy.theme.e4_default">
          </property>
          <property
                name="rendererFactoryUri"
                value="bundleclass://org.xmind.cathy/org.xmind.cathy.internal.renderer.XWorkbenchRendererFactory">
          </property>
          <property
                name="modelResourceHandler"
                value="bundleclass://org.xmind.cathy/org.xmind.ui.internal.e4handlers.XResourceHandler">
          </property>
      </product>
   </extension>
   <extension
         point="org.eclipse.core.runtime.preferences">
      <initializer
            class="org.xmind.cathy.internal.CathyPrefInitializer">
      </initializer>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
      		id="org.xmind.ui.prefPage.General"
            name="%prefPage.general.name">
      </page>

      <page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
            id="org.xmind.ui.prefPage.Share"
            name="%prefPage.share.name">
      </page>
      <page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
            id="org.xmind.ui.prefPage.Spelling"
            name="%prefPage.spelling.name">
      </page>
      <page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
            id="org.xmind.ui.prefPage.Language"
            name="%prefPage.language.name">
      </page>
      <page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
            id="org.xmind.ui.prefPage.Advanced"
            name="%prefPage.advanced.name">
      </page>
      <page
            class="org.eclipse.ui.ExtensionFactory:newKeysPreferencePage"
            id="org.eclipse.ui.preferencePages.Keys"
            name="%prefPage.keys.name">
      </page>
      <!--<page
            class="org.xmind.cathy.internal.GeneralPrefPage"
            id="org.xmind.ui.prefPage.General"
            name="%prefPage.general.name">
      </page>
      <page
            class="org.xmind.cathy.internal.LanguagePrefPage"
            id="org.xmind.ui.LanguagePrefPage"
            name="%prefPage.language.name">
      </page>-->
      <!--<page
            class="org.xmind.ui.preference.MultiSectionPreferencePage"
            id="org.xmind.ui.ProxyPreferencePage"
            name="Proxy">
      </page>-->
      <!--
      <page
            name="%prefPage.appearance.name"
            category="org.xmind.ui.prefPage.General"
            class="org.eclipse.ui.ExtensionFactory:colorsAndFontsPreferencePage"
            id="org.eclipse.ui.preferencePages.ColorsAndFonts">
      </page>
      -->
   </extension>
   <extension
         point="org.eclipse.ui.views">
      <view
            category="org.eclipse.ui"
            class="org.eclipse.ui.ExtensionFactory:progressView"
            icon="icons/views/pview.gif"
            id="org.eclipse.ui.views.ProgressView"
            name="%view.progress.name"
            restorable="true">
      </view>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            categoryId="org.xmind.ui.category.cloud"
            description="%command.showHome.description"
            id="org.xmind.ui.command.showDashboard"
            name="%command.showHome.name">
         <commandParameter
               id="org.xmind.ui.command.showDashboard.pageId"
               name="%commandParameter.homePage.name"
               optional="true">
         </commandParameter>
      </command>
      <command
            categoryId="org.xmind.ui.category.cloud"
            description="%command.toggleHome.description"
            id="org.xmind.ui.command.toggleDashboard"
            name="%command.toggleHome.name">
      </command>
      <command
            categoryId="org.eclipse.ui.category.help"
            description="%command.welcome.description"
            id="org.xmind.ui.command.welcome"
            name="%command.welcome.name">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.bindings">
      <key
            commandId="org.eclipse.ui.window.showKeyAssist"
            contextId="org.eclipse.ui.contexts.window"
            schemeId="org.eclipse.ui.defaultAcceleratorConfiguration"
            sequence="M1+M2+L">
      </key>
   </extension>
   <extension
         point="org.eclipse.ui.startup">
      <startup
            class="org.xmind.cathy.internal.AutoSaveService">
      </startup>
   </extension>
   <extension
         point="org.eclipse.ui.handlers">
      <handler
            class="org.xmind.cathy.internal.handlers.ShowAllPreferencesHandler"
            commandId="org.eclipse.ui.window.preferences">
         <activeWhen>
            <with
                  variable="activeWorkbenchWindow">
               <instanceof
                     value="org.eclipse.ui.IWorkbenchWindow">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.WelcomeToXMindHandler"
            commandId="org.xmind.ui.command.welcome">
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.ShowDashboardPageHandler"
            commandId="org.xmind.ui.command.showDashboard">
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.ToggleDashboardHandler"
            commandId="org.xmind.ui.command.toggleDashboard">
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.NewWizardHandler"
            commandId="org.eclipse.ui.newWizard">
         <activeWhen>
            <with
                  variable="activeWorkbenchWindow">
               <instanceof
                     value="org.eclipse.ui.IWorkbenchWindow">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.HideDashboardHandler"
            commandId="org.eclipse.ui.file.closePart">
         <activeWhen>
            <with
                  variable="activePartId">
               <equals
                     value="org.xmind.cathy.part.dashboard">
               </equals>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="org.xmind.cathy.internal.handlers.XMindHelpHandler"
            commandId="org.eclipse.ui.help.helpContents">
         <activeWhen>
            <with
                  variable="activeWorkbenchWindow">
               <instanceof
                     value="org.eclipse.ui.IWorkbenchWindow">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
      <handler
            class="org.xmind.ui.internal.wizards.WizardHandler$Export"
            commandId="org.eclipse.ui.file.export">
         <activeWhen>
            <with
                  variable="activeWorkbenchWindow">
               <instanceof
                     value="org.eclipse.ui.IWorkbenchWindow">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
            <handler
            class="org.xmind.ui.internal.wizards.WizardHandler$Import"
            commandId="org.eclipse.ui.file.import">
         <activeWhen>
            <with
                  variable="activeWorkbenchWindow">
               <instanceof
                     value="org.eclipse.ui.IWorkbenchWindow">
               </instanceof>
            </with>
         </activeWhen>
      </handler>
   </extension>
   <extension
         point="org.xmind.core.command.handlers">
      <handler
            class="org.xmind.cathy.internal.OpenFileCommandHandler"
            id="org.xmind.ui.xmindCommandHandler.openFile"
            command="^openFile$">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.statusHandlers">
      <statusHandler
            class="org.xmind.cathy.internal.CathyStatusHandler"
            id="org.xmind.ui.statusHandler">
      </statusHandler>
      <statusHandlerProductBinding
            handlerId="org.xmind.ui.statusHandler"
            productId="org.xmind.cathy.product">
      </statusHandlerProductBinding>
   </extension>
